{
  "name": "things-ive-built",
  "license": "GPL-3.0",
  "version": "0.1.0",
  "scripts": {
    "dev": "yarn kill-port 3000 && nodemon",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "test": "yarn type-check && (yarn kill-port 3000 &) && (yarn dev &) && wait-on http://localhost:3000 && yarn cypress run && (yarn kill-port 3000 &)",
    "test:i": "((yarn dev &) || true) && wait-on http://localhost:3000 && yarn cypress open",
    "deploy": "yarn test && yarn build && now --public && now alias things-ive-built.com"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@react-firebase/auth": "^0.2.7",
    "@react-firebase/database": "^0.3.7",
    "classnames": "^2.2.6",
    "express": "^4.16.3",
    "firebase": "^6.3.1",
    "firebase-admin": "^8.2.0",
    "immer": "^3.1.3",
    "initialize-firebase-app": "^1.1.0",
    "is-url": "^1.2.4",
    "lodash": "^4.17.10",
    "material-ui-chip-input": "^1.0.0-beta.7",
    "next": "^9.3.2",
    "now-env": "^3.1.0",
    "react": "^16.8.6",
    "react-art": "^16.5.0",
    "react-component-component": "^1.2.1",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.8.6",
    "react-microlink": "^2.2.4",
    "render-and-add-props": "^0.5.0",
    "styled-components": "^3.4.5"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/styled-jsx": "^2.2.5",
    "concurrently": "^4.0.1",
    "cypress": "^3.1.0",
    "cypress-testing-library": "^2.2.1",
    "kill-port": "^1.3.2",
    "prettier": "^1.14.2",
    "typescript": "^3.5.3",
    "wait-on": "^3.0.1"
  }
}
